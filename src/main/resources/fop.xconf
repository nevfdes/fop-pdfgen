<fop version="1.0">
	<renderers>
		<renderer mime="application/pdf">
			<filterList>
				<!-- provides compression using zlib flate (default is on) -->
				<value>flate</value>
			</filterList>
			<fonts>
				<!-- detect OS fonts -->
				<auto-detect />
			</fonts>
		</renderer>
	</renderers>
</fop>